    "import pandas as pd\n",
    "import altair as alt\n",
    "\n",
    "# Define the data\n",
    "data = pd.DataFrame(\n",
    "    {\"category\": [\"Frogs\", \"Hogs\", \"Dogs\", \"Logs\"], \"value\": [15, 30, 45, 10]}\n",
    ")\n",
    "\n",
    "alt.Chart(data).mark_arc().encode(theta=\"value\", color=\"category\")"
